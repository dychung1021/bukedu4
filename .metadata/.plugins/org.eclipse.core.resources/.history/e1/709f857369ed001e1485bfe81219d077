package test;

public class test1 {
	public static void main(String[] args) {
	// for 문을 이용해서 1~10까지 순서대로 출력하시오.
		for (int i=1; i<11; i++) {
			System.out.print(i+"\t");
		}//for
		//*
		//**
		//***
		//****
		//*****
		//for문을 이용하여 출력
		for (int i=1; i<=5;i++) {
			for (int j=1;j<=i;j++) {
			System.out.print("*");
		}//inner for
			System.out.println();
		}//outer for
	}//main
}//class
